/*---------------------------------------------------------------------------------------------------*/

/* Layout pour les √©crans de moins de 800px de largeur 
*
* Layout responsive - si √©cran de moins de 800px de largeur, 
* le side et le main passent l'un en dessous de l'autre au lieu de l'un √† c√¥t√© de l'autre 
*/
@media only screen and (max-width: 800px) {
	.container {
	  flex-direction: column;
	}
	
	.side, .main {
	  flex: none;
	  width: 100%;
	}
  }



/* Layout responsive - si √©cran de moins de 600px de largeur, 
le side et le main passent l'un en dessous de l'autre au lieu de l'un √† c√¥t√© de l'autre
Les ingr√©dients passent en icone √† ouvrir */

/* Responsive */
@media only screen and (max-width: 600px) {
	/* Header */
	.header {
	  flex-direction: column;
	  height: auto;
	}
	.accueil {
	  width: 100%;

	}
	.accueil .mascotte {
	  display: none;
	}
	.accueil .titre {
	  width: 100%;
	}
	.boite_recherche {
	  width: 100%;
	}

	.iconenav {
		margin-top: 5px;
	}

	#boutonLoupe {
	  left: auto;
	  right: 10px;
	}

	/* Menu Gauche */
	.menuGauche {
	  width: 100%;
	  height: auto;
	  position: relative;
	  margin-top: 20px;
	  box-shadow: none;
	}
	.side {
	  width: 100%;
	  border-right: none;
	  padding-top: 0px;
	  flex-direction: row;
	}
	.side1, .side2, .side3 {
	  height: auto;
	  width: 33.33%;
	}
	.saisieIngredient {
	  margin: 5px 5px 10px 5px;
	}
  }

/* Layout responsive - si √©cran de moins de 360px de largeur, le side et le main passent l'un en dessous de l'autre au lieu de l'un √† c√¥t√© de l'autre */

@media (max-width: 900px) {
	.row, .navbar {   
		flex-direction: column;
	}
  
	#fondDroiteHeader{
		background-color:#52A8B4;
	}
  
	#fondGaucheHeader{
		border-style: none;
	}
	
	h1{
		text-align:center;
		font-size:20px
		/*peut le rendre tr√®s petit ou le supp quand gros zoom et agrandir la barre de recherche et icones a la place ou mettre les trucs les uns en dessous des autres*/
	}
	
	#containerHeader{
		flex-direction:column;
	}
	
	.side {
		padding-top: 150px;
		border:none;
	}

	.main {
		padding-top: 150px;
		
	}
	
	.navbar{
		flex-direction:row;
		justify-content:start;
		align-items:center;
	}
	
}

@media (max-width: 1100px) {
	
	.colonneGest{
		flex-direction:row;
		justify-content: space-between;
	}
	
	.panneauSide{
		height:100%;
	}
	
	.lienPageModif{
		flex: 1 1 0;
	}
	
	.categ{
		font-size:100%;
	}
	
	.imageRefRI{
		width : 75%;
	}
	
}

@media (max-width: 400px) {
	
	.bouton{
		font-size:70%;
	}
	
	.adaptBouton{
		font-size:50%;
	}
}

@media (max-width: 300px) {
	
	.bouton{
		font-size:70%;
	}
	
	.adaptBouton{
		font-size:40%;
	}
}
/* test Recette -------------------------------------------------- */

.recette {
	
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
	width: 90%;
	margin-bottom: 20px;
	box-shadow: 0 4px 6px rgba(0,0,0,0.1);
	overflow: hidden;
	background-color: #9d9d9d;
	border-radius: 15px; /* ajoute des bords arrondis */
  }
  
  .recette:nth-child(3n+1) {
	margin-right: 0;
  }
  
  .RecetteGauche {
	width: 50%;
	padding: 10px;
	box-sizing: border-box;
	background-color: #9d9d9d;
  }
  
  .RecetteGauche h2 {
	font-size: 20px;
	margin: 0;
	padding-right: 10px;
  }
  
  .recette-img {
	width: 100%;
	height: 0;
	padding-bottom: 56.25%;
	position: relative;
	overflow: hidden;
	background-color: white;
	border-radius: 20px;
	z-index: 0;
  }
  
  .recette-img img {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	object-fit: cover;
  }
  
  .RecetteGauche p {
	margin: 0;
	font-size: 14px;
  }
  
  .RecetteDroite {
	width: 30%;
	background-color: #fff;
	background-color: #9d9d9d;
  }
  
  .recette-info {
	padding: 10px;
	border-bottom: 1px solid #ddd;
  }
  
  .recette-difficulte,
  .recette-temps {
	margin-bottom: 10px;
  }
  
  .recette-difficulte span,
  .recette-temps span {
	display: block;
	font-size: 14px;
	font-weight: bold;
	margin-bottom: 5px;
  }
  
  .listeIng {
	list-style: none;
	padding: 0;
	margin: 0;
  }
  
  .listeIngSolo {
	font-size: 14px;
	padding: 5px 10px;
	border-bottom: 1px solid #ddd;
  }

  .contentR {
	display: flex;
	flex-wrap: wrap;
	width: 100%;
	height: 100%;
  }
  
  .R1 {
	width: 40%;
	height: 40%;
	order: 1;
	margin-right: 10px;

  }
  
  .R2 {
	width: 40%;
	height: 40%;
	order: 3;
	margin-right: 10px;
  }
  
  .R3 {
	width: 40%;
	height: 40%;
	order: 2;
	margin-right: 10px;
  }
  
  .R4 {
	width: 40%;
	height: 40%;
	order: 4;
	margin-right: 10px;
  }