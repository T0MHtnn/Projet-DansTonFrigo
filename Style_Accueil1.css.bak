@font-face {
	font-family: "Milky Coffee";
	src: url("fonts/milky_coffee/Milky_Coffee.ttf");
}

@font-face {
	font-family: "Handlee";
	src: url("fonts/handlee/Handlee-Regular.ttf");
}

* {
  box-sizing: border-box;
}

/* Style du body */
body {
	font-family: Arial;
	margin: 0;
	display: flex;
	width: 100%;
	height: 100%;
	background-color: white;
	/* overflow: hidden; */
	position: relative;
}

.centrageTexte {
	text-align: center;
}

/*---------------------------------------------------------------------------------------------------*/
/* Header */

.header {
	font-family: "Milky Coffee", sans-serif;
	background: #F58E54;
	color: black;

	position: fixed;

	display: flex;
	flex-direction: row;
	height:80px;
	width: 100%;

	box-shadow: 0px 0px 10px #000,
	0px 0px 30px #777;
	
	border-bottom-left-radius:10px;
	border-bottom-right-radius:10px;

	z-index: 2;
}

.accueil {
	width: 30%;
	display: flex;
}

/* Mascotte */

.accueil .mascotte {
	text-align: center;
	overflow: visible;
	width : 20%;
}

.image_mascotte {
	z-index: -1;
	width: 75%;
	height: 98px;
}

.image_mascotte2 {
	z-index: -1;
	width: 75%;
	height: 115px;
}

.accueil .titre{
	text-align: center;
	color:black;
	padding:3px;
	text-shadow: 1px 1px 2px grey;
	width: 80%;
}

.boite_recherche{
	width: 40%;
	margin: 0px;
}


.lienDiscret{
	text-decoration:none;
	color:black;	
}

/* barre de recherche */

#boutonLoupe { 
	position: absolute; 
	left:791px;
	top:14px;
	
	border: none;
	background-color: rgba(255,255,255,0);
	width:60px;
	height:40px;
	
	border-radius:25px;
}

#loupe{
	width:80%;
}

.centrage{
	display: flex; /* contexte sur le parent */
	flex-direction: column; /* direction d'affichage verticale */
	justify-content: center;
	align-items: stretch;
}

.saisieRecette{
	font-family: "Handlee", sans-serif;
	
	font-weight: bold;
 	position: relative;
	
	width:100%;
	height: 40px;
	
	font-size: 14px;
	
	background-color:white;

	box-shadow: 0px 0px 5px #000,
                0px 0px 15px #777;
				
	border-radius: 25px;
	border: none;
	border-bottom: 1px solid hsl(0, 0%, 90%);
	padding-left:10px;

	z-index: 0;
	outline: none;
}

/* icones cliquables */

.navbar {
	flex:3.5 1 0;
	display: flex;
	flex-direction:row;
	flex-wrap: wrap;
	justify-content:end;
}


.iconenav{
	flex:1 1 0;
	display: flex;
	flex-direction:column;
	flex-wrap: wrap;
	
	color:black;
	text-decoration:none;
	
	position: relative;
	padding: 0 3px;
	transition: 0.4s ease;
}

.iconenav:hover{
	color:#ccc;
	
}

.iconenav::before{
	content: '';
	width: 100%;
	height: 0%;
	border-radius:10px;
	background: #fff;
	position: absolute;
	bottom: 0;
	left: 0;
	transition: 0.4s ease;
	
}

.iconenav:hover::before{
	height:100%;
	mix-blend-mode:difference;
	
}

.icone_texte{
	text-align:center;
	object-fit:contain;
	padding-top: 0px;
	margin-top: 5px;
	margin-bottom: 0px;
}

.image_icone {
	width:40%;
	aspect-ratio:3/2;
	object-fit:contain;
	margin-left:auto;
	margin-right:auto;
	margin-bottom: 0px;
	margin-top: -5px;
	padding-top: 5px;
	padding-bottom: 0px;
}

.lien_icone {
	width:20%;
	margin: 10px;
}


/*---------------------------------------------------------------------------------------------------*/

/* menuGauche */
.menuGauche {
	background-color: #52A8B4;

	flex-direction: column;
	width: 20%;
	margin-top: 80px;
	margin-bottom: -80px;
	height: 100%;
	position: fixed;
	z-index: 1;

	box-shadow: 0px 0px 5px #000,
				0px 0px 15px #777;
}

.side {

	background-color: #52A8B4;
	
	padding-top: 70px;
	border-right: solid #A3A3A3 4px; 

	font-family: "Handlee", sans-serif;

	width: 20%;
	display: flex;
	height: 100%;
}


.side1 {
	height: auto;
	font-family: "handlee", sans-serif;
}

.side2 {
	height: 50%;
}

.side2-2 {
	height: 70%;
	overflow-y: scroll;
	overflow:auto;
}

.side2-2::-webkit-scrollbar {
  width: 20px; /* width of the entire scrollbar */
}

.side2-2::-webkit-scrollbar-track {
  background: #52A8B4; /* color of the tracking area */
  border-radius: 40px;
}

.side2-2::-webkit-scrollbar-thumb {
  background-color: #A4A9A4; /* color of the scroll thumb */
  border-radius: 40px; /* roundness of the scroll thumb */
  border: 5px solid #52A8B4;
}

.side3 {
	height: auto;
	width: 100%;
}

.saisieIngredient {

