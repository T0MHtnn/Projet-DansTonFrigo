/* boutons stylis√©s */
.bouton {
  position: relative;
  display: inline-block;
  margin: 1%;
  padding: 0.5% 2%;
  text-align: center;
  font-size: 100%;
  letter-spacing: 1px;
  text-decoration: none;
  color: black;
  background: transparent;
  cursor: pointer;
  transition: ease-out 0.5s;
  border: 2px solid black;
  border-radius: 10px;
  box-shadow: inset 0 0 0 0 white;
}

.bouton:hover {
  color: white;
  border:2px solid white;
  box-shadow: inset 0 -100px 0 0 #52A8B4;
}

.bouton:active {
  transform: scale(0.9);
}



#texteRetourAccueil {
  color: white;
  text-decoration: none;
  font-size: 100%;
  border: none;
  background: none;
  font-weight: 600;
  font-family: 'Poppins', sans-serif;
}

#texteRetourAccueil::before {
  margin-left: auto;
}

#texteRetourAccueil::after, #texteRetourAccueil::before {
  content: '';
  width: 0%;
  height: 2px;
  background: white;
  display: block;
  transition: 0.5s;
}

#texteRetourAccueil:hover::after, #texteRetourAccueil:hover::before {
  width: 100%;
}

.souligner{
	text-decoration:underline;
}

